{
  "name": "@uncover/js-utils",
  "version": "0.0.7",
  "author": "aSH <ash.uncover@gmail.com>",
  "description": "javascript utils",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/ash-uncover/js-utils"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\" \"src/**/*.js\" \"src/**/*.jsx\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test-coverage && npm run lint",
    "preversion": "npm run lint",
    "test": "jest --env=jsdom",
    "test-coverage": "jest --env=jsdom --collectCoverage",
    "version": "npm run format"
  },
  "devDependencies": {
    "@types/jest": "^29.2.1",
    "@types/node": "^18.11.9",
    "jest": "^29.2.2",
    "jest-environment-jsdom": "^29.2.2",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.8.4"
  }
}
